<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.diaristaslimpo.limpo">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Limpo - Diarista"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Controler.telas.TelaLogin"
            android:theme="@style/SemBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controler.telas.TelaSEndereco" />
        <activity android:name=".Controler.telas.TelaServico" />
        <activity android:name=".Controler.telas.TelaSDataServico" />
        <activity android:name=".Controler.telas.TelaSolicitacoes" />
        <activity android:name=".Controler.telas.TelaDetalheSolicitacao" />
        <activity android:name=".Controler.telas.TelaDadosPessoais" />
        <activity
            android:name=".Controler.telas.TelaInicial"
            android:label="@string/title_activity_tela_inicial"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Controler.telas.TelaCadastro1" />
        <activity android:name=".Controler.telas.TelaCadastroEndereco" />
        <activity android:name=".Controler.telas.Tela_Historico" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".Controler.telas.Tela_DetalheHistorico" />
        <activity android:name=".Controler.telas.Tela_MeusEnderecos" />
        <activity android:name=".Controler.telas.Tela_Prox_Diarias" />
        <activity android:name=".Controler.telas.TelaAvalicao"></activity>
    </application>

</manifest>